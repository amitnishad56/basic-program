# c++ program to print  number  
  
#include<iostream>
using namespace std;

int main(){

int number;

cout<<"Enter an integer";
cin>>number; 

cout<<"\nThe Number entered is :: " << number<<"\n";
  
return 0;
}
 
 # c++ program to addition of  two number
 
 #include<iostream>
using namespace std;

int main(){

    int a, b, sum;

    cout<<"enter the first integer";
    cin>>a;
    cout<<"enter second integer";
    cin>>b;
    sum=a+b;
    cout<<sum;

    return 0;

}
